{
    "aggregations": [
        {
            "metric": "io.confluent.kafka.server/request_count"
        }
    ],
    "filter": {
        "field": "resource.kafka.id",
        "op": "EQ",
        "value": "lkc-XXXXX"
    },
    "granularity": "PT1H",
    "group_by": [
        "metric.principal_id"
    ],
    "intervals": [
        "2023-06-25T00:00:00Z/PT1H"
    ]
}