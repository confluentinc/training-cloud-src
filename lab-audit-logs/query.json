{
    "aggregations": [
        {
            "metric": "io.confluent.kafka.server/received_bytes"
        }
    ],
    "filter": {
        "field": "resource.kafka.id",
        "op": "EQ",
        "value": "lkc-XXXXX"
    },
    "granularity": "PT1M",
    "group_by": [
        "metric.topic"
    ],
    "intervals": [
        "2025-06-25T09:00:00Z/2025-06-25T12:00:00Z"
    ]
}